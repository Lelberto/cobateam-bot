{
  "name": "cobateam-bot",
  "version": "1.0.0",
  "description": "Discord bot for the CobaTeam server",
  "main": "build/app.js",
  "scripts": {
    "start": "nodemon",
    "build": "tsc",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lelberto/cobateam-bot.git"
  },
  "author": "Lelberto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lelberto/cobateam-bot/issues"
  },
  "homepage": "https://github.com/Lelberto/cobateam-bot#readme",
  "dependencies": {
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^13.13.0",
    "@types/ws": "^7.2.4",
    "nodemon": "^2.0.20",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "ext": "ts"
  }
}
